package helloword;

class PersonInfo{
	String name;
	int age;
	double height;
	PersonInfo(){}
}
public class Hellow{
	public static void main(String[] args) {
		PersonInfo p = new PersonInfo();
		p.name = "Alice";
		p.age = 12;
		p.height = 120.5;
		System.out.println("p.name:"+p.name);
		System.out.println("p.age:"+p.age);
		System.out.println("p.height:"+p.height);
	}
}



p.name:Alice
p.age:12
p.height:120.5
