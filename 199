package helloword;

public class Hellow{
	public static void main(String[] args) {
		int a = 5, b=0, c;
		int A[] = new int[3];
		try {
			A[3]=100;
			c=a/b;
		}
		catch(ArithmeticException e) {
			System.out.println("0으로 나눌 수 없다");
		}
		catch(Exception e) {
			System.out.println("없는 인데스");
		}
		finally {
			System.out.println("finally");
		}
	}
}
