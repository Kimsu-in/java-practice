package helloword;

public class Hellow{
	public static void main(String[] args) {
		java.util.Scanner scin = new java.util.Scanner(System.in);
		System.out.print("Enter your name:");
		String name = scin.next();
		System.out.println("You are "+name);
		scin.close();
	}
}


Enter your name:suin
You are suin

