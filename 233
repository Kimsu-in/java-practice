package helloword;

import java.util.ArrayList;

public class Hellow {
 	public static void main(String[] args) {
 		ArrayList<String> number = new ArrayList<String>();
 		number.add("one");
 		number.add("two");
 		number.add("three");
 		number.add("four");
		
 		for(int i=0;i<number.size();i++)
 			System.out.println(number.get(i));
	}
}
