package helloword;

class Dog{
	String name;
	String color;
	
	Dog(){
		System.out.println("Default constructor is called");
	}
	void printDog() {
		System.out.println("name:"+name);
		System.out.println("color:"+color);
	}
}
public class Hellow
{
	public static void main(String[] args) {
		Dog d=new Dog();
		d.printDog();
	}
}



Default constructor is called
name:null
color:null
